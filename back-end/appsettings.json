{
  "ConnectionStrings": {
    "FoodOrder": "${FOODORDER_DB_CONNECTION_STRING}"
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpiryDurationInMinutes": "${JWT_EXPIRY_DURATION_IN_MINUTES}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "${LOGGING_DEFAULT}",
      "Microsoft.AspNetCore": "${LOGGING_MICROSOFT_ASPNETCORE}"
    }
  },
  "PaypalOptions": {
    "AppId": "${PAYPAL_APP_ID}",
    "AppSecret": "${PAYPAL_APP_SECRET}",
    "Mode": "${PAYPAL_MODE}"
  },
  "VnPay": {
    "TmnCode": "${VNPAY_TMNCODE}",
    "HashSecret": "${VNPAY_HASH_SECRET}",
    "BaseUrl": "${VNPAY_BASE_URL}",
    "Version": "${VNPAY_VERSION}",
    "Command": "${VNPAY_COMMAND}",
    "CurrCode": "${VNPAY_CURR_CODE}",
    "Locale": "${VNPAY_LOCALE}",
    "PaymentBackReturnUrl": "${VNPAY_PAYMENT_BACK_RETURN_URL}"
  },
  "GoogleKeys": {
    "ClientId": "${GOOGLE_CLIENT_ID}",
    "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
  },
  "AllowedHosts": "${ALLOWED_HOSTS}"
}
